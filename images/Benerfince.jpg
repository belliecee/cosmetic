<script type="text/javascript">
jQuery(document).ready(function(){	

	jQuery('input.shizadow').removeClass('textinput'); 

	// Attach keypress handler to input box.  Submit form if user presses 'enter' key.
	jQuery('.email').keypress( function(e) {
		if(e.which == 13) {
			jQuery('#sendBtn').click();
			return false;
		}
		return true;
	});
	
	jQuery('#sendBtn').click( function() {
		
		jQuery('.email').find('.errormessage').remove();
		var emailField = 'dwfrm_emailsignup_emailAddress';
		
		if( !isValidEmailAddress(jQuery('#'+ emailField).val()) )
		{						
			jQuery('.email').find('.value').append('<span class="errormessage">Please enter a valid Email Address</span>');
			return;
		}		
          		
        cm_emailSignup(jQuery('#'+ emailField).val());
               
		jQuery('.formactions').append("<input name='dwfrm_emailsignup_signup' type='hidden' />");
		var formId = 'dwfrm_emailsignup';			
		var post = jQuery('#'+ formId).serialize();
 
		jQuery.ajax({
		   type: 'POST',
		   url: '/on/demandware.store/Sites-Shiseido_US-Site/en_US/SHNewsletter-SubscribeNewsletter',
		   data: post,
		   dataType: 'html',
		   success: function(data){			   	   		
			   app.dialog.open('/on/demandware.store/Sites-Shiseido_US-Site/en_US/SHNewsletter-ThankYou', ' ');
			   jQuery('#'+ emailField).val('');	   

		   },
		   failure: function(data) {
			   alert('error');		
		   }
		});		
	});

	//Assign source value in email Sign Up to send to Exact Target
	jQuery('.emailSignUpContainerEmail #dwfrm_emailsignup_source').attr('value','header');
	
});

function isValidEmailAddress(emailAddress) {
	var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
	return pattern.test(emailAddress);
}
