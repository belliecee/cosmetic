<script type="text/javascript">
	function htmlEncode(value){
		return jQuery("<div/>").html(value).text();
	}

	/*
		disableStuff(), enableButtons(), and disableButtons() are used by color-select.isml and size-select.isml
	*/
	function disableStuff()
	{
		// Disable based on size select if one exists.
		if (jQuery("input[type='radio']").length > 0 || jQuery(".colorVariationSelect").length > 0)
		{
			if(jQuery("input[type='radio']:checked").hasClass("soldout") || jQuery("select.colorVariationSelect option:selected").hasClass("soldout"))
				disableButtons();
			else
				enableButtons();
		}
		else
		{
			var inStock = true;
			if (inStock)
				enableButtons();
			else
				disableButtons();
		}
		if(jQuery("select.colorVariationSelect option:selected").hasClass("soldout"))
		{
			jQuery(".OutofStockColorMessage").show();
		}
		else
		{
			jQuery(".OutofStockColorMessage").hide();
		}
	}

	function enableButtons()
	{
		//enable qty drop down   
		jQuery(".quantityinput").removeAttr("disabled");
		//enable Add To Bag(cart) Button
		jQuery(".addtocartbutton").removeAttr("disabled");
		//change the Add to Bag Button Image to represent active. 
		jQuery("button#addtocart").removeClass("disabled");
	}

	function disableButtons()
	{
		//disable qty drop down  
		jQuery(".quantityinput").attr("disabled","disabled");
		//disable Add To Bag(cart) Button
		jQuery(".addtocartbutton").attr("disabled","disabled");
		//change add to bag(cart) image to represent disabled
		jQuery("button#addtocart").addClass("disabled");
	}
	
</script>

